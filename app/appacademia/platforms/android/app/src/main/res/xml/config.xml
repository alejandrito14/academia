<?xml version='1.0' encoding='utf-8'?>
<widget id="com.woolis.app" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
    </feature>
    <feature name="CustomFCMReceiverPlugin">
        <param name="android-package" value="cordova.plugin.customfcmreceiver.CustomFCMReceiverPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <allow-navigation href="cdvfile:*" />
    <feature name="FileTransfer">
        <param name="android-package" value="org.apache.cordova.filetransfer.FileTransfer" />
    </feature>
    <feature name="FirebasePlugin">
        <param name="android-package" value="org.apache.cordova.firebase.FirebasePlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Geolocation">
        <param name="android-package" value="org.apache.cordova.geolocation.Geolocation" />
    </feature>
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>
    <feature name="Media">
        <param name="android-package" value="org.apache.cordova.media.AudioHandler" />
    </feature>
    <feature name="NetworkStatus">
        <param name="android-package" value="org.apache.cordova.networkinformation.NetworkManager" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <name>Woolis</name>
    <description>
        Aplicacion de academias
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-navigation href="https://*.stripe.network/*" />
    <allow-navigation href="https://*.stripe.com/*" />
    <allow-navigation href="https://*.issoftware1.com.mx/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <access launch-external="yes" origin="whatsapp:*" />
    <access launch-external="yes" origin="whatsapp://*" />
    <resource-file src="res/android/build-extras.gradle" target="app/build-extras.gradle" />
    <resource-file src="res/android/gradle.properties" target="gradle.properties" />
    <resource-file src="res/android/raw/blackberry.mp3" target="app/src/main/res/raw/blackberry.mp3" />
    <resource-file src="res/android/raw/msn.mp3" target="app/src/main/res/raw/msn.mp3" />
    <resource-file src="res/android/raw/crystal.mp3" target="app/src/main/res/raw/crystal.mp3" />
    <resource-file src="res/android/drawable-mdpi/ic_launcher.png" target="app/src/main/res/drawable-mdpi/ic_launcher.png" />
    <resource-file src="res/android/drawable-hdpi/ic_launcher.png" target="app/src/main/res/drawable-hdpi/ic_launcher.png" />
    <resource-file src="res/android/drawable-xhdpi/ic_launcher.png" target="app/src/main/res/drawable-xhdpi/ic_launcher.png" />
    <resource-file src="res/android/drawable-xxhdpi/ic_launcher.png" target="app/src/main/res/drawable-xxhdpi/ic_launcher.png" />
    <resource-file src="res/android/drawable-xxxhdpi/ic_launcher.png" target="app/src/main/res/drawable-xxxhdpi/ic_launcher.png" />
    <resource-file src="res/android/drawable-xxxhdpi/ic_launcher_large.png" target="app/src/main/res/drawable-xxxhdpi/ic_launcher_large.png" />
    <resource-file src="res/android/values/colors.xml" target="app/src/main/res/values/colors.xml" />
    <allow-intent href="market:*" />
    <icon src="res/icons/android/playstore-icon.png" />
    <splash density="land-ldpi" src="res/icons/android/drawable-land-ldpi-screen.png" />
    <splash density="land-mdpi" src="res/icons/android/drawable-land-mdpi-screen.png" />
    <splash density="land-hdpi" src="res/icons/android/drawable-land-hdpi-screen.png" />
    <splash density="land-xhdpi" src="res/icons/android/drawable-land-xhdpi-screen.png" />
    <splash density="land-xxhdpi" src="res/icons/android/drawable-land-xxhdpi-screen.png" />
    <splash density="land-xxxhdpi" src="res/icons/android/drawable-land-xxxhdpi-screen.png" />
    <splash density="port-ldpi" src="res/icons/android/800x1340.png" />
    <splash height="800" src="res/icons/android/1340x800.png" width="1340" />
    <splash height="1340" src="res/icons/android/800x1340.png" width="800" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
    <preference name="SplashScreenDelay" value="5000" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="AndroidXEnabled" value="true" />
    <preference name="AndroidInsecureFileModeEnabled" value="true" />
    <preference name="loadUrlTimeoutValue" value="700000" />
    <preference name="android-minSdkVersion" value="22" />
</widget>
